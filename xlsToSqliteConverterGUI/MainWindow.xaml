<Window x:Class="Lofle.XlsToSqliteConverterGUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lofle.XlsToSqliteConverterGUI"
        mc:Ignorable="d"
        Title="Drag &amp; Drop" Height="183" Width="256" Drop="DropFile" AllowDrop="true" ResizeMode="NoResize">
    <Grid Margin="0,0,0,0">
        <ProgressBar x:Name="convertProgress" HorizontalAlignment="Left" Height="20" Margin="41,97,0,0" VerticalAlignment="Top" Width="166"/>
        <ProgressBar x:Name="insertProgress" HorizontalAlignment="Left" Height="20" Margin="41,68,0,0" VerticalAlignment="Top" Width="166"/>
        <CheckBox x:Name="checkBox" HorizontalAlignment="Left" Height="26" Margin="41,32,0,0" VerticalAlignment="Top" Width="166" Checked="checkBox_Checked" RenderTransformOrigin="0.518,1.857" >
            <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="21" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="146"/>
        </CheckBox>
    </Grid>
</Window>
